import java.util.Stack;


public class StackBot {
	
	final int NORTH = 0;
	final int SOUTH = 1;
	final int EAST = 2;
	final int WEST = 3;
	
	Stack<String> instructions = new Stack<String>();
	Stack<Float> distance = new Stack<Float>();
	Stack<String> direction = new Stack<String>();
	
	public static void main(String[] args){
		new StackBot();
	}
	
	public StackBot(){
		instructions.push("34.2m W");
		direction.push(Character.toString(instructions.pop().charAt(6)));
		for(int i = 0; i < 4; i++){
			float d = distance.peek(((Float.parseFloat(Character.toString(instructions.peek().charAt(i))))));
			System.out.println(d);
		}
		
	}
	
}
